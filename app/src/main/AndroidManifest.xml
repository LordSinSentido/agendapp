<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.classhub.agendapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Agendapp">
        <activity android:name=".MostrarEventoActivity"></activity>
        <activity android:name=".AgregarHorariosActivity" />
        <activity android:name=".AgregarEventosActivity" />
        <activity android:name=".AgregarTareasActivity" />
        <activity android:name=".ConfiguracionesActivity" />
        <activity android:name=".MenuPrincipalActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>